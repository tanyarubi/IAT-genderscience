						<Choice value=".">.</Choice>
				<Choice value="5">Extremely important</Choice>
				<Choice value="4">Very important</Choice>
				<Choice value="3">Somewhat important</Choice>
				<Choice value="2">Slightly important</Choice>
				<Choice value="1">Not at all important</Choice>
