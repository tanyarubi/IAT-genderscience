<Questionnaire ID="1" name="GenderScience">
	<Block Name="block1" columns="2">
		<Text>Please rate how much you associate the following domains with males or females.</Text>
		<Question type="choice" render="dropselect" ID="Lscience" required="">
			<Text>Science</Text>
			<Choices>
				<Choice value=".">.</Choice>
				<Choice value="7">Strongly male</Choice>
				<Choice value="6">Moderately male</Choice>
				<Choice value="5">Slightly male</Choice>
				<Choice value="4">Neither male nor female</Choice>
				<Choice value="3">Slightly female</Choice>
				<Choice value="2">Moderately female</Choice>
				<Choice value="1">Strongly female</Choice>
			</Choices>
		</Question>
		<Question type="choice" render="dropselect" ID="Larts" required="">
			<Text>Liberal Arts</Text>
			<Choices>
				<Choice value=".">.</Choice>
